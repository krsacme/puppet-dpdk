{
  "name": "openstack-dpdk",
  "version": "0.0.1",
  "author": "OpenStack Contributors",
  "summary": "Puppet module for OpenStack Dpdk",
  "license": "Apache-2.0",
  "source": "git://github.com/openstack/puppet-dpdk.git",
  "project_page": "https://launchpad.net/puppet-dpdk",
  "issues_url": "https://bugs.launchpad.net/puppet-dpdk",
  "description": "Installs and configures OpenStack Dpdk.",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": ["8"]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": ["21","22"]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["7"]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["14.04"]
    }
  ],
  "dependencies": [
    { "name": "puppetlabs/inifile", "version_requirement": ">=1.0.0 <2.0.0" },
    { "name": "puppetlabs/stdlib", "version_requirement": ">= 4.2.0 <5.0.0" },
    { "name": "openstack/openstacklib", "version_requirement": ">=9.0.0 <10.0.0" },
    { "name": "openstack/oslo", "version_requirement": ">=9.0.0 <10.0.0" }
  ]
}
